<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.ufrn.eaj.tads.meuslivros">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/book"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/book"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PrincipalActivity" android:screenOrientation="locked" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CadastroActivity" android:screenOrientation="locked" />
        <activity android:name=".ListaLivrosActivity" android:screenOrientation="locked" />
        <activity android:name=".AutoCompleteActivity" android:screenOrientation="locked" />
        <activity android:name=".ListViewActivity" android:screenOrientation="locked" />
        <activity android:name=".RecyclerActivity" android:screenOrientation="locked" />
    </application>

</manifest>